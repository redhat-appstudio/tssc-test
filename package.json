{
    "name": "tssc-test",
    "version": "1.0.0",
    "description": "TSSC Test is a testing framework for TSSC API using Playwright and TypeScript.",
    "scripts": {
        "lint": "eslint --ext .ts src tests",
        "lint:fix": "eslint --ext .ts src tests --fix",
        "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
        "check-types": "tsc --noEmit",
        "validate": "npm run lint && npm run format && npm run check-types",
        "test": "playwright test",
        "test:report": "playwright show-report",
        "test:tssc": "playwright test tests/tssc/full_workflow.test.ts",
        "test:ui": "playwright test tests/tssc/ui.test.ts",
        "ui": "playwright test --ui tests/tssc/ui.test.ts"
    },
    "keywords": [
        "typescript",
        "playwright",
        "testing",
        "api"
    ],
    "author": "TSSC Team",
    "license": "Apache License 2.0",
    "dependencies": {
        "@gitbeaker/rest": "^42.5.0",
        "@janus-idp/shared-react": "^2.18.0",
        "@types/async-retry": "^1.4.9",
        "async-retry": "^1.3.3",
        "p-retry": "^6.2.1",
        "sodium-native": "^5.0.4",
        "typescript": "^5.8.3"
    },
    "devDependencies": {
        "@backstage/plugin-scaffolder-react": "^1.16.0",
        "@kubernetes/client-node": "^1.2.0",
        "@octokit/plugin-retry": "^8.0.1",
        "@octokit/plugin-throttling": "^11.0.1",
        "@octokit/rest": "^22.0.0",
        "@playwright/test": "^1.52.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "^20.12.14",
        "@types/sodium-native": "^2.3.9",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "axios": "^1.9.0",
        "dotenv": "^16.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.0",
        "glob": "^11.0.2",
        "nock": "^14.0.4",
        "otplib": "^12.0.1",
        "pino": "^9.7.0",
        "pino-pretty": "^13.0.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}